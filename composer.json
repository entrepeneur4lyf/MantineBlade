{
    "name": "phpstack/mantine-blade",
    "description": "A Lareavel Livewire 3 Component library based on Mantine",
    "type": "library",
    "version": "0.0.1",
    "authors": [
        {
            "name": "Shawn McAllister",
            "email": "dev@phpstack.dev"
        }
    ],
    "require": {
        "illuminate/support": "^11.30",
        "illuminate/foundation": "^11.30",
        "ijpatricio/mingle": "^0.0.18",
        "jfcherng/php-diff": "^6.16",
        "laravel/prompts": "^0.3.1",
        "blade-ui-kit/blade-icons": "^1.7",        
        "ryangjchandler/blade-tabler-icons": "^2.3",
        "ueberdosis/tiptap-php": "^1.4"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^3.2",
        "pestphp/pest": "^3.5",
        "livewire/livewire": "^3.0"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "MantineBlade\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "MantineBlade\\MantineBladeServiceProvider"
            ],
            "aliases": {
                "MantineBlade": "MantineBlade\\Facades\\MantineBlade"
            }
        }
    },
    "minimum-stability": "stable",
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
